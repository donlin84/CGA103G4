<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.member.model.MemberVO" table="member">
	
		<id name="memid" column="memid">
			<generator class="identity" />
		</id>
		
		<property name="memName" column="memName" />
		<property name="memAccount" column="memAccount" />
		<property name="memPassword" column="memPassword" />
		<property name="memGender" column="memGender" />
		<property name="memPhone" column="memPhone" />
		<property name="memEmail" column="memEmail" />
		<property name="memAddres" column="memAddres" />
		<property name="memBirthday" column="memBirthday" />
		<property name="memStatus" column="memStatus" />
		<property name="memNation" column="memNation" />
		
	</class>
</hibernate-mapping>

